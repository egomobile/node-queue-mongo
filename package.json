{
    "name": "@egomobile/queue-mongo",
    "version": "0.2.0",
    "description": "Queue storage for node-queue, using MongoDB as backend.",
    "main": "lib/index.js",
    "engines": {
        "node": ">=18.0.0"
    },
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "lib",
        "!lib/**/*.map",
        "README.md"
    ],
    "keywords": [
        "node",
        "js",
        "ts",
        "javascript",
        "typescript",
        "queue",
        "list",
        "task",
        "job",
        "priority",
        "db",
        "database",
        "ensure",
        "execution",
        "background",
        "storage",
        "agenda",
        "mongodb",
        "nosql",
        "extension",
        "plugin",
        "class",
        "types",
        "tools",
        "helpers",
        "utilities",
        "ego"
    ],
    "scripts": {
        "build": "del ./lib && tsc",
        "docs:generate": "del ./docs && typedoc --out ./docs ./src",
        "lint": "eslint -c .eslintrc.js --ext .ts src",
        "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
        "sandbox": "nodemon --watch './sandbox/index.ts' ./sandbox/index.ts --inspect=0.0.0.0:9229"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/egomobile/node-queue-mongo"
    },
    "author": "Next.e.GO Mobile SE, Aachen, Germany",
    "license": "LGPL-3.0",
    "devDependencies": {
        "@egomobile/queue": "~3.3.0",
        "@egomobile/tsconfig": "^5.0.0",
        "@types/node": "18.19.3",
        "del-cli": "5.1.0",
        "dotenv": "16.0.3",
        "eslint": "8.55.0",
        "eslint-config-ego": "^0.19.0",
        "mongodb": "^5.3.0",
        "nodemon": "2.0.20",
        "ts-node": "10.9.1",
        "typedoc": "0.23.21",
        "typescript": "4.7.4"
    },
    "peerDependencies": {
        "@egomobile/queue": "^3.3.0",
        "mongodb": ">=5.3.0 <6"
    }
}