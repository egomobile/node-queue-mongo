{
  "name": "@egomobile/queue-mongo",
  "version": "0.1.2",
  "description": "Queue storage for node-queue, using MongoDB as backend.",
  "main": "lib/index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "lib",
    "!lib/**/*.map",
    "README.md"
  ],
  "keywords": [
    "node",
    "js",
    "ts",
    "javascript",
    "typescript",
    "queue",
    "list",
    "task",
    "job",
    "priority",
    "db",
    "database",
    "ensure",
    "execution",
    "background",
    "storage",
    "agenda",
    "mongodb",
    "nosql",
    "extension",
    "plugin",
    "class",
    "types",
    "tools",
    "helpers",
    "utilities",
    "ego"
  ],
  "scripts": {
    "build": "del ./lib && tsc",
    "docs:generate": "del ./docs && typedoc --out ./docs ./src",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "nodemon --watch './sandbox/index.ts' ./sandbox/index.ts --inspect=0.0.0.0:9229"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egomobile/node-queue-mongo"
  },
  "author": "Next.e.GO Mobile SE, Aachen, Germany",
  "license": "LGPL-3.0",
  "dependencies": {
    "@types/node": "14.18.31"
  },
  "devDependencies": {
    "@egomobile/tsconfig": "^5.0.0",
    "@egomobile/queue": "~3.3.0",
    "del-cli": "5.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.29.0",
    "eslint-config-ego": "^0.17.1",
    "mongodb": "^4.0.0",
    "nodemon": "2.0.20",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "typedoc": "0.23.21",
    "typescript": "4.6.4"
  },
  "peerDependencies": {
    "@egomobile/queue": "^3.3.0",
    "mongodb": "^4.0.0"
  }
}
